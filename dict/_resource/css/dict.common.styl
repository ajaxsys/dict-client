.__dict__ hr
    border 0
    height 1px
    background-image -webkit-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
    background-image    -moz-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
    background-image     -ms-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))
    background-image      -o-linear-gradient(left, rgba(0,0,0,0), rgba(0,0,0,0.75), rgba(0,0,0,0))


/* Bootstrap Extend */
.navbar
    min-width 280px

.nav-header
    font-weight bold


/* My page classes */
.__navi_div__
    border-radius 3px;
    background-color rgba(255,255,255,0.5)
    cursor pointer

.__navi_on__
    background sprite(go_on)

.__navi_off__
    background sprite(go_off)

.__window_go_top__
    background sprite(go_top)
    cursor pointer

.__window_go_next__
    background sprite(go_next)
    cursor pointer

.__window_go_back__
    background sprite(go_back)
    cursor pointer


/* My page IDs */
COMMON_FONT_SIZE = 14px
COMMON_FONT_FAMILY = "Helvetica Neue",Helvetica,Arial,sans-serif
COMMON_FONT = COMMON_FONT_SIZE COMMON_FONT_FAMILY

#__navi__ a
    text-decoration none
    font COMMON_FONT
    
#__navi__ input
    font COMMON_FONT

#__explain_wrapper__
    margin-top 20px

#__search_form__
    margin-right 50px

#__search__
    position relative
    z-index 9999
    top 6px

#__dict_types__
    width:100%




